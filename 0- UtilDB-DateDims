/// Load of initial Gregorian Date Calendar
create or replace TABLE UTIL_DB.PUBLIC.DATEDIMS (
	"DATEDIMS_ID%" NUMBER(38,0) NOT NULL,
	DATE DATE NOT NULL,
	DAYOFMONTH_NUMBER NUMBER(38,0) NOT NULL,
	DAYOFWEEK_NUMBER NUMBER(38,0) NOT NULL,
	DAYOFYEAR_NUMBER NUMBER(38,0) NOT NULL,
	DAYOFWEEK_SHORTNAME VARCHAR(3) NOT NULL,
	DAYOFWEEK_LONGNAME VARCHAR(10) NOT NULL,
	WEEK_KEY NUMBER(38,0) NOT NULL,
	IS_WEEKEND BOOLEAN NOT NULL,
	WEEKOFYEAR_NUMBER NUMBER(38,0) NOT NULL,
	WEEKOFMONTH_NUMBER NUMBER(38,0) NOT NULL,
	WEEK_START DATE NOT NULL,
	WEEK_END DATE NOT NULL,
	MONTH_KEY NUMBER(38,0) NOT NULL,
	MONTH_NUMBER NUMBER(38,0) NOT NULL,
	MONTH_SHORTNAME VARCHAR(3),
	MONTH_LONGNAME VARCHAR(10),
	MONTH_START DATE NOT NULL,
	MONTH_END DATE NOT NULL,
	QUARTER_KEY NUMBER(38,0) NOT NULL,
	QUARTER_NUMBER NUMBER(38,0) NOT NULL,
	QUARTER_NAME VARCHAR(3) NOT NULL,
	QUARTER_START DATE NOT NULL,
	QUARTER_END DATE NOT NULL,
	YEAR_NUMBER NUMBER(38,0) NOT NULL,
	primary key (DATE)
);
